FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 5e-07;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 
0
;
    }
    outlet
    {
        type            zeroGradient;
    }
    waterplane
    {
        type            symmetryPlane;
    }
    bottom
    {
        type            symmetryPlane;
    }
    side
    {
        type            symmetryPlane;
    }
    midPlane
    {
        type            symmetryPlane;
    }
    hull
    {
        type            nutkRoughWallFunction;
        Ks              uniform 0.0001;
        Cs              uniform 0.5;
        value           uniform 5e-07;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 5e-07;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 5e-07;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 5e-07;
    }
    procBoundary1to4
    {
        type            processor;
        value           uniform 5e-07;
    }
    procBoundary1to7
    {
        type            processor;
        value           uniform 5e-07;
    }
}


