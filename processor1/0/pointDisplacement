FoamFile
{
    format      binary;
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 
0
;
    }
    outlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 
0
;
    }
    waterplane
    {
        type            symmetryPlane;
    }
    bottom
    {
        type            symmetryPlane;
    }
    side
    {
        type            symmetryPlane;
    }
    midPlane
    {
        type            symmetryPlane;
    }
    hull
    {
        type            calculated;
    }
    procBoundary1to0
    {
        type            processor;
    }
    procBoundary1to2
    {
        type            processor;
    }
    procBoundary1to3
    {
        type            processor;
    }
    procBoundary1to4
    {
        type            processor;
    }
    procBoundary1to7
    {
        type            processor;
    }
}


